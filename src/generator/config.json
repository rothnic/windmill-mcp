{
  "version": "1.0.0",
  "openapi": {
    "specUrl": "https://app.windmill.dev/api/openapi.json",
    "backupUrl": "https://raw.githubusercontent.com/windmill-labs/windmill/main/openapi.json",
    "localCache": "./cache/openapi-spec.json",
    "cacheExpiry": 86400
  },
  "generator": {
    "tool": "openapi-mcp-generator",
    "outputDir": "./build",
    "tempDir": "./build/temp",
    "preserveFiles": [
      "build/index.js",
      "build/config.js"
    ]
  },
  "templates": {
    "customDir": "./src/generator/templates",
    "useCustom": false
  },
  "postGeneration": {
    "applyOverrides": true,
    "runLinter": true,
    "runTests": false,
    "generateDocs": false
  },
  "overrides": {
    "sourceDir": "./src/overrides",
    "strategy": "merge",
    "conflictResolution": "manual",
    "backupOnConflict": true
  },
  "logging": {
    "level": "info",
    "file": "./cache/logs/generation.log",
    "console": true
  }
}
